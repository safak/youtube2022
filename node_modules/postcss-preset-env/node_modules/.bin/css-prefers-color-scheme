#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../css-prefers-color-scheme/dist/cli.cjs" "$@"
  ret=$?
else 
  node  "$basedir/../../../css-prefers-color-scheme/dist/cli.cjs" "$@"
  ret=$?
fi
exit $ret
